# This is a sample Caddyfile. You can edit this file to add your own services.
# You can use [caddy-self-hosted](https://github.com/AiratTop/caddy-self-hosted)
# in the same `shared_network`

# WordPress blog service.
blog.example.com {
	reverse_proxy wp:80 {
		flush_interval -1
	}
}

# phpMyAdmin service.
pma.example.com {
	reverse_proxy pma:80 {
		flush_interval -1
	}
}